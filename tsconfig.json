{
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "jsx": "react-jsx",
    "target": "es2017",
    "downlevelIteration": true,
    "moduleResolution": "bundler",
    "baseUrl": ".",
    "paths": {
      "@ui/*": ["mods/ui/*"],
      "@mods/*": ["mods/*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "src/types/env.d.ts",
    "nativewind-env.d.ts"
  ],
  "extends": "expo/tsconfig.base"
}
